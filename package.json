{
  "name": "gitlab-env-generator",
  "version": "0.1.6",
  "description": "gitlab-env-generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/StekolschikovV/gitlab-env-generator"
  },
  "main": "lib/index",
  "types": "lib/index",
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "start": "ts-node src/index.ts --env=.env",
    "test": "npx mocha",
    "watch": "npx mocha -w",
    "build": "npx tsc -p ."
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "watch-files": [
      "src/**/*.ts",
      "test/**/*.ts"
    ],
    "require": "ts-node/register"
  },
  "author": "Vitaly StekolschikovV v.a.stekolschikov@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^18.11.9",
    "chai": "^4.3.4",
    "eslint": "^7.25.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.4.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.36",
    "typedoc-plugin-markdown": "^3.8.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "axios": "^1.1.3"
  }
}
